{
  "name": "semester-project2",
  "version": "0.0.6",
  "description": "A retro auction site for vintage LP's",
  "type": "module",
  "main": "scripts/index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "watch": "sass --watch src/scss:dist/css & live-server",
    "format": "prettier -w scripts/**/*.js",
    "lint": "eslint scripts/**/*.js",
    "lint-fix": "eslint scripts/**/*.js --cache --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Murots/Semester-project2.git"
  },
  "keywords": [
    "ccs",
    "bootstrap",
    "sass",
    "js",
    "html",
    "website",
    "auction",
    "lp"
  ],
  "author": "Aleksander Storum <AleSto95654@stud.noroff.no>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Murots/Semester-project2/issues"
  },
  "homepage": "https://github.com/Murots/Semester-project2#readme",
  "dependencies": {
    "bootstrap": "^5.3.2"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "live-server": "^1.2.2",
    "prettier": "^3.1.0",
    "sass": "^1.69.5"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
